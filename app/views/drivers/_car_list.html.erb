<table class="table table-striped">
  <thead>
    <tr>
      <th><b>Vehicle Reg</b></th>
      <th><b>Make</b></th>
      <th><b>Model</b></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% cars.each do |c| %>
      <tr>
        <td><%= c.vehicle_reg %></td>
        <td><%= c.make%></td>
        <td><%= c.model%></td>
        <td>
          <div  style="margin-top: -5px; margin-left: -5px">
            <%= link_to '#', class: 'btn btn-success' do %>
                Data Stream
            <% end %>
          </div>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
